//begin header
+b.HEADER.header(class= headerClass)
	+e.container.container
		+e.A.logo(href='#!')
			+e.IMG(src='img/other/logo.png' alt='Aura')

		+b.nav
			+e.BUTTON.btn(type='button')
				+e.SPAN.btn-burger
					+e.SPAN.btn-lines
			+e.NAV.body
				-
					navList = [
					['How to start', '#!'],
					['Pricing', '#!'],
					['Sign in', '#!']
					]
				+e.UL.list
					for navLink, i in navList
						+e.LI.list-item
							if i === 2
								+e.A.list-link._login(href=`#${navLink[1]}`)= navLink[0]
									+i('long-arrow-right')
							else
								+e.A.list-link(href=`#${navLink[1]}`)= navLink[0]
//end header
